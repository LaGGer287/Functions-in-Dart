import 'dart:math';
import 'dart:io';

void pytago() {
  double a;
  double b;
  print("Enter a: ");
  a = double.parse(stdin.readLineSync()!);
  print("Enter b: ");
  b = double.parse(stdin.readLineSync()!);
  if (a > 0 && b > 0) {
    double c = sqrt(a * a + b * b);
    print("$a^2 + $b^2 = $c^2");
  }
}

void main() {
  pytago();
}